{
  "name": "@club/api",
  "version": "0.1.0",
  "description": "Backend API for badminton club management",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx src/server.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:clean": "rm -rf ./tsconfig.tsbuildinfo && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:prod": "NODE_ENV=production rm -rf ./tsconfig.tsbuildinfo && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "test": "NODE_ENV=test jest --config jest.config.ts",
    "seed:dev": "NODE_ENV=development MONGODB_URI_DEV=mongodb://localhost:27017/badminton-club-demo-dev tsx src/scripts/seedData.ts",
    "seed:test": "NODE_ENV=test MONGODB_URI_TEST=mongodb://localhost:27017/badminton-club-test tsx src/scripts/seedData.ts",
    "seed:prod": "NODE_ENV=production tsx src/scripts/seedData.ts",
    "db:reset": "tsx src/scripts/seedData.ts",
    "db:reset:dev": "NODE_ENV=development MONGODB_URI_DEV=mongodb://localhost:27017/badminton-club-demo-dev tsx src/scripts/seedData.ts",
    "db:reset:test": "NODE_ENV=test MONGODB_URI_TEST=mongodb://localhost:27017/badminton-club-demo-test tsx src/scripts/seedData.ts",
    "db:clear": "tsx src/scripts/clearDatabase.ts",
    "db:clear:dev": "NODE_ENV=development MONGODB_URI_DEV=mongodb://localhost:27017/badminton-club-demo-dev tsx src/scripts/clearDatabase.ts",
    "db:clear:test": "NODE_ENV=test MONGODB_URI_TEST=mongodb://localhost:27017/badminton-club-demo-test tsx src/scripts/clearDatabase.ts"
  },
  "dependencies": {
    "@club/shared-types": "workspace:*",
    "@pdf-lib/fontkit": "^1.1.1",
    "@types/debug": "^4.1.12",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-cron": "^4.2.1",
    "pdf-lib": "^1.17.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@testcontainers/mongodb": "^11.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.19.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
  }
}