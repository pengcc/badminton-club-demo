###############################################
# Web Environment Example (.env.local)
###############################################

# Base API URL (used by axios client & server components)
NEXT_PUBLIC_API_URL=http://localhost:3003/api

# Dev-time rewrite origin (Next.js rewrites only in development)
DEV_API_ORIGIN=http://localhost:3003

# Show demo credentials banner on login page
NEXT_PUBLIC_SHOW_DEMO_HINTS=true
